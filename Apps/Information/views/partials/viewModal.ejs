 <!-- Modal -->
 <div class="modal fade" id="modelId3" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
    <div class="modal-dialog  modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Edit Record</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
            </div>
            <div class="modal-body">
                <form action="/" method="post">

                    <div class="row">
                        <div class="col">
                            <div class="my-3 text-start">
                                <label class="lead">Full Name</label>
                                <input type="text" name="fullName" class="form-control">
                            </div>                            
                        </div>
                        <div class="col">
                           <div class="my-3 text-start">
                               <label class="lead">Date of birth</label>
                               <input type="date" name="dateOfBirth" class="form-control">
                           </div>                            
                        </div>
                    </div>

                    <div class="my-2 text-start">
                        <label class="lead">Gender</label>
                        <div class="row">

                            <!-- <div class="col">
                                <div class="form-check">
                                  <input class="form-control col-5" type="text" name="gender">
                                  <label class="form-check-label" for="flexRadioDefault2">
                                    Gender
                                  </label>
                                </div>                                
                            </div> -->
                            
                            <div class="col">
                                  <div class="form-check">
                                      <input class="form-check-input" type="radio" name="gender" value="Male" id="flexRadioDefault1">
                                      <label class="form-check-label" for="flexRadioDefault1">
                                        Male
                                      </label>
                                  </div>                                
                            </div>
                            <div class="col">
                                <div class="form-check">
                                  <input class="form-check-input" type="radio" name="gender" value="Female" id="flexRadioDefault2" checked>
                                  <label class="form-check-label" for="flexRadioDefault2">
                                    Female
                                  </label>
                                </div>                                
                            </div>

                            <div class="col">
                                <div class="form-check">

                                    <%
                                     let date = new Date()
                                     let year = date.getFullYear(); 
                                     %>

                                    <label class="lead">Age</label>
                                  <input class="form-control col-5" type="text" value="<%= year %>"  name="age">
                                </div>                                
                            </div>

                        </div>
                    </div>


                    <div class="row">
                        <div class="col">
                            <div class="my-3 text-start">
                                <label class="lead">Residence</label>
                                <input type="text" name="residence" class="form-control">
                            </div>
                        </div>

                        <div class="col">
                            <div class="my-3 text-start">
                                <label class="lead">Profession</label>
                                <input type="text" name="proffession" class="form-control">
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col">       
                            <div class="my-3 text-start">
                                <label class="lead">Email</label>
                                <input type="email" name="email" class="form-control">
                            </div>
                        </div>

                        <div class="col">
                            <div class="my-3 text-start">
                                <label class="lead">Upload Image</label>
                                <input type="file" name="image" class="form-control">
                            </div>
                        </div>
                    </div>


                    <div class="my-3 text-start">
                        <label class="lead">Description</label>
                        <textarea class="form-control" name="bio" cols="20" rows="10"></textarea>
                    </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="submit" id="edit" class="btn text-white" >Edit Record</button>
            </div>

                </form>
            </div>

        </div>
    </div>
</div>

<script>
    const addBtn = document.querySelector('#edit');
    addBtn.addEventListener('click', (element) => {
        element.preventDefault();

    })
</script>